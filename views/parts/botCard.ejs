.row .col {
    text-align: center;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    padding: 0 .75rem;
    min-height: 1px;
    display: inline-table;
    float: none;
}

.card .card-content {
    padding-top: 24px;
    padding-left: 5px;
    padding-right: 5px;
    border-radius: 0 0 2px 2px;
}

.container{
    max-width: 3000px;
    text-align: center;
    margin-left: auto;
    margin-right: auto;
}

.mo {
  display: none;
  position: fixed;
  z-index: 1;
  padding-top: 100px;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%; 
  overflow: auto;
}

.mc {
  position: relative;
  margin: auto;
  padding: 0;
  border: 1px solid;
  width: 80%;
  box-shadow: 0 4px 8px 0 rgba(0,0,0,2),0 6px 20px 0 rgba(0,0,0,2);
  -webkit-animation-name: animatetop;
  -webkit-animation-duration: 0.4s;
  animation-name: animatetop;
  animation-duration: 0.4s
}


@-webkit-keyframes animatetop {
  from {top:-300px; opacity:0} 
  to {top:0; opacity:1}
}

@keyframes animatetop {
  from {top:-300px; opacity:0}
  to {top:0; opacity:1}
}

.c {
  color: white;
  float: right;
  font-size: 80px;
  font-weight: bold;
}

.c:hover,
.c:focus {
  color: #7289DA;
  text-decoration: none;
  cursor: pointer;
}

.mh {
  padding: 2px 16px;
  background-color: #2C2F33;
  color: white;
}

#modal-body1 {padding: 16px;}
#modal-body2 {padding: 16px;}
#modal-body3 {padding: 16px;}

.mod {
