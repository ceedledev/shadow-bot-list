        <%- include("../parts/headerpre2") %>
        <div class="main">
            
    
                <% if (rawBots.length == 0) { %>
                    <p style="color: #bdc3c7;margin-top: 25px;text-align: center;font-size: 29px;">No bot :( </p>
                <% } else { %>          
                    <div class="list-bot">  
                        <div style="display: inline-block;vertical-align: middle;margin-left: 20px;">
                        <img style="vertical-align: middle;" src="https://cdn.discordapp.com/emojis/790896547939221504.png?v=1" width="60px" height="60px">
                        </div>
    
                        <div style="display: inline-block;vertical-align: middle;">
                        <div class="title-cat">
                        New bot
                        </div>     
                        <div class="title-cat-sub">
                            New bots added on the site
                        </div>   
                        </div>      
           
                     <%- botChunks.map(chunk => `<div>${chunk.map(bot => include("../parts/botv2card", {bot})).join("")}</div>`).join("") %>
                     
                     <div style="" class="center-align">
                        <ul class="pagination center-align">
                            <% if (page === 0) { %>
                            <li class="disabled"><a><i class="material-icons grey-text text-darken-2">chevron_left</i></a></li>
                            <% } else { %>
                            <li class="waves-effect"><a href="?page=<%= page %>"><i
                                        class="material-icons grey-text text-lighten-1">chevron_left</i></a></li>
                            <% } %>
        
                            <% for (let p = 0; p < pages; p++) { %>
                            <li class="<%= page === p ? 'active grey darken-1' : 'waves-effect' %>"><a <%- page===p ? '' :
                                    `href="?page=${p + 1}" class="grey-text text-lighten-1"
                                    ` %> >
                                    <%= p + 1 %></a></li>
                            <% } %>
        
                            <% if (page === pages - 1) { %>
                            <li class="disabled"><a><i class="material-icons grey-text text-darken-2">chevron_right</i></a></li>
                            <% } else { %>
                            <li class="waves-effect"><a href="?page=<%= page + 2 %>"><i
                                        class="material-icons grey-text text-lighten-1">chevron_right</i></a></li>
                            <% } %>
                        </ul>
        
                     </div>
              </div>
         
         </div>
                <% } %>

               
            </div>
        </div>
