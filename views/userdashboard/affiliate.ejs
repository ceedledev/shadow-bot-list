    <style>
        body{
            font-family: 'Quicksand', sans-serif;
            background-color: transparent;
        }


        .main{
            background-color: transparent;
            flex-direction: column;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -ms-flex-direction: column;
        }

        .leftmenu{
            background-color: #1d1f21;
            display: -webkit-box;
            display: -ms-flexbox;
            display: flex;
            -webkit-box-orient: vertical;
            -webkit-box-direction: normal;
            -ms-flex-direction: column;
            
            overflow-x: hidden;
            padding: 15px;
            font-family: 'Quicksand', sans-serif; 

            flex-direction: column;
            min-height: 0;
            width: 350px;
            -webkit-box-flex: 0;
            -ms-flex: 0 0 auto;
            flex: 0 0 auto;
 
    
        }

        .rightmenu{
            background-color: #222428;
            font-family: 'Quicksand', sans-serif;
            width: 100%;
            overflow-x: hidden;
            -webkit-box-orient: vertical;
            -webkit-box-direction: normal;
            -ms-flex-direction: column;
            flex-direction: column;
            max-height: fit-content;
            display: -webkit-box;
            display: -ms-flexbox;
            display: flex;
            padding: 15px;
        } 

        .dashboard{ 
            display: -webkit-box;
            display: -ms-flexbox;
            display: flex;
            -webkit-box-align: stretch;
            -ms-flex-align: stretch;
            align-items: stretch;
            -webkit-box-pack: start;
            -ms-flex-pack: start;
            justify-content: flex-start;
            min-width: 0;
            min-height: 0;
            -webkit-box-flex: 1;
            -ms-flex: 1 1 auto;
            flex: 1 1 auto;
            height: 100vh;
        }

        .menu-phone{
            display: none;
            cursor: pointer;
            width: max-content;
        }

        @media (max-width:900px){
            .leftmenu{
                display: none;
            }

            .menu-phone{
                display: block;
            }
        }


        @media (min-width: 900px){
            
            .dashboard{ 
                display: -webkit-box;
            display: -ms-flexbox;
            display: flex;
            -webkit-box-align: stretch;
            -ms-flex-align: stretch;
            align-items: stretch;
            -webkit-box-pack: start;
            -ms-flex-pack: start;
            justify-content: flex-start;
            min-width: 0;
            min-height: 0;
            -webkit-box-flex: 1;
            -ms-flex: 1 1 auto;
            flex: 1 1 auto;
            height: 100vh;
            }
        }

        .btn-list{
            width: 100%;
            height: 100%;
            padding-left: 15px;
            padding-right: 15px;
            padding-top: 10px;
            padding-bottom: 10px;
        }

        .dashboard-header{
            background:transparent;
            width: max-content;
            cursor: pointer;
        }

        .dashboard-header:hover{
            opacity: .80;
        }

        .btn-dashboard{
            background-color: transparent;
            color: rgb(184, 184, 184);
            width: 100%;
            height: max-content;
            padding-top: 10px;
            padding-bottom: 10px;
            border-radius: 4px;
            cursor: pointer;
            padding-left: 15px;
            margin-bottom: 10px;
        }
        .btn-dashboard:hover{
            color: white;
        }

        .btn-dashboard-active{
            background-color: #7289DA;
            color: white;
        }
        .title-btn-dashboard{
            color: rgb(226, 226, 226);
            font-size: 13px;
            margin-left: 15px;
            padding-bottom: 10px;
            text-transform: uppercase;
        }

        .new-card{
            background-color: #43b581;
            width: max-content;
            height: max-content;
            padding-top: 3px;
            padding-bottom: 3px;
            padding-left: 5px;
            padding-right: 5px;
            border-radius: 3px;
            font-size: 13px;
            margin-left: 5px;
            vertical-align: top;
            color: white;
        }

        .new-card-active{
            background-color: white;
            color: #232926;
            width: max-content;
            height: max-content;
            padding-top: 3px;
            padding-bottom: 3px;
            padding-left: 5px;
            padding-right: 5px;
            border-radius: 3px;
            font-size: 13px;
            margin-left: 5px;
            vertical-align: top;
        }

        .title-page{
            color: white;
            font-size: 22px;
        }

        .title-sub{
            color: grey;
            margin-top: 10px;
            font-size: 18px;
        }

        .user-card-profil{
            background-color: #202325;
            border:solid 1px #0f1011;
            width: 350px;
            height: max-content;
            padding-top: 10px;
            padding-bottom: 10px;
            padding-left: 10px;
            padding-right: 10px;
            margin-top: 15px;
            border-radius: 6px;
            position: relative;
            margin-top: 35px;
        }

        .btn-profil{
            background-color: #7289DA;
            width: max-content;
            height: max-content;
            padding-top: 5px;
            padding-bottom: 5px;
            padding-left: 10px;
            padding-right: 10px;
            color: white;
            text-align: center;
            position: absolute;
            border-radius: 4px;
            top: 50%;
            right: 10px;
            transform: translate(-10%, -50%);
            cursor: pointer;
        }

        .card-user-info{
            background-color: #202325;
            border:solid 1px #0f1011;
            width: 200px;
            height: max-content;
            padding-top: 15px;
            padding-bottom: 15px;
            padding-left: 15px;
            padding-right: 15px;
            display: inline-block;
            margin-right: 20px;
            border-radius: 4px;
            margin-top: 25px;
        }

        .card-info-title{
            color: white;
            text-transform: uppercase;
        }
        .card-info-sub{
            color: grey;
        }
        .btn-card-info{
            background-color: #7289DA;
            width: 100%;
            height: max-content;
            padding-top: 7px;
            padding-bottom: 7px;
            padding-left: 10px;
            padding-right: 10px;
            text-align: center;
            color: white;
            text-transform: uppercase;
            margin-top: 10px;
            border-radius: 4px;
            cursor: pointer;
        }

        .premium-btn{
            background-color: #43b581;
        }

        .off{
            display: none !important;
        }

        .on{
            display: block !important;
        }

        .pourcentall{
            width: 100% !important;
        }

        .user-profil{
            cursor: pointer;
            background-color: transparent;
            width: 100%;
            position: relative;
            margin-bottom: 30px;
            margin-top: 20px;
        }

        .angledown{
            position: absolute;
            top: 50%;
            right: 0;
            transform: translate(0%, -50%);
        }

        .userdrop{
            background-color: #0f1011;
            position: absolute;
            top: 150%;
            border-radius: 4px;
            right: 15px;
            left: 15px;
            height: max-content;
            box-shadow: 0px 0px 5px  #141414;
        }

